class Solution {
public:
    bool containsDuplicate(vector<int>& nums) 
    {
        int n=nums.size(),c=0;
        sort(nums.begin(),nums.end());
        for(int i=0;i<n-1;i++)
        {
                if(nums[i]==nums[i+1])
                {
                    c++;
                    break;
                }
                
        }
        if(c==1)
            return true;
        else
            return false;
    }
};
