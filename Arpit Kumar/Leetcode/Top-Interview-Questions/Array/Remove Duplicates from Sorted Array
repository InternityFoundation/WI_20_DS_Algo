class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
    int x=0;
    if(nums.size()==0) return 0;
    for(int i=0;i<nums.size()-1;i++)
    {
        if(nums[i]!=nums[i+1]){
            nums[x]=nums[i];
            x++;
        }
    }
    nums[x]=nums[nums.size()-1];
    return x+1;   
    }
};
