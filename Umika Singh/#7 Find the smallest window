#include <iostream>
#include <bits/stdc++.h>
using namespace std;
void unsorted(vector<int>& arr, int n) 
{ 
int s = 0, e = n-1, i, max, min;  
for (s = 0; s < n-1; s++) 
{ 
    if (arr[s] > arr[s+1]) 
    break; 
} 
if (s == n-1) 
{ 
    cout << "The array is already sorted"; 
    return; 
} 
for(e = n - 1; e > 0; e--) 
{ 
    if(arr[e] < arr[e-1]) 
    break; 
} 
max = arr[s]; min = arr[s]; 
for(i = s + 1; i <= e; i++) 
{ 
    if(arr[i] > max) 
    max = arr[i]; 
    if(arr[i] < min) 
    min = arr[i]; 
} 
for( i = 0; i < s; i++) 
{ 
    if(arr[i] > min) 
    {  
    s = i; 
    break; 
    }      
}  
for( i = n -1; i >= e+1; i--) 
{ 
    if(arr[i] < max) 
    { 
    e = i; 
    break; 
    }  
}  
cout<< s <<" "<< e; 
return; 
} 
  
int main() {
	// your code goes here
	vector<int> arr;
	int n, d;
	cin>>n;
	for(int i=0;i<n;i++)
	{
	    cin>>d;
	    arr.push_back(d);
	}
   unsorted(arr,n);
	return 0;
}
