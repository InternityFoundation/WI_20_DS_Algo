#include<bits/stdc++.h>
using namespace std;

vector<string> split_string(string S){
    
    
    char *s;
     s = &S[0];
    vector<string>str;
    char *token = strtok(s, " "); 
     
    while (token != NULL) 
    { 
        string token_string(token);
        
        str.push_back(token_string);
         
        token = strtok(NULL, " "); 
    }
    return str;
}


int main (int argc, char** argv){
    
 
   fstream fin; 
    vector<string>usernames,colleges;
    fin.open(argv[0]);
    string temp,line,word;
    string username,college;
    int count=0;
        while (fin >> temp) { 
        count=0;
        getline(fin, line); 

        stringstream s(line); 
        
        
        while (getline(s, word, ', ')) { 
            if(count==1)username=word;
            else if(count==4)college=word;
            count++;
            
        }
        
        usernames.push_back(username);
        colleges.push_back(college);
}

for(auto &i:colleges){
    if(i=="Shri Ram Murti Smarak College of Engg. and Tech" || i=="SRMS CET")i="SRMS";
    else if(i=="IIT Delhi" || i=="Indian insstitute of technology Delhi")i="IIT D";
   else  if(i=="IIIT Delhi" || "Indian insstitute of information technology Delhi")i="IIIT D";
   else if(i=="NIT Agartala")i="NITAgt";
   else if(i=="NIT Allahabad")i="NITA";
}

vector<string>s;

for(auto &i:colleges){
    
    s=split_string(i);
    for(auto &j:s){
        for(auto &k:j)k=toupper(k);
    }
}
int n=colleges.size();
bool vis[n];
for(int i=0;i<n;i++){
    vis[i]=false;
}
for(int i=0;i<n;i++){
    if(vis[i])continue;
    for(int j=i;j<n;j++){
        if(colleges[i]==colleges[j])
        {
            cout<<username[j]<<",";
            vis[j]=true;
        }
    }
    cout<<"\n";
}


}
